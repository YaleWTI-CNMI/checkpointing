#!/bin/bash
#SBATCH -p gpu_test -q qos_test
#SBATCH --time=1:00:00
#SBATCH --gres=gpu:a40:1
#SBATCH -n 1 --ntasks-per-node=1 
#SBATCH --reservation=workshop
