module load miniconda
conda activate torch_2.4
[ "x${SLURM_RESTART_COUNT}" == "x" ] && SLURM_RESTART_COUNT=0
srun python ex5.py > ex5.out.${SLURM_JOB_ID}.${SLURM_RESTART_COUNT}
